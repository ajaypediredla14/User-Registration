<!DOCTYPE html>
<html lang="en">
<head>
	<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
	<link href="https://fonts.googleapis.com/css2?family=Jost:wght@500&display=swap" rel="stylesheet">
</head>
<body>
<div class="main1">
<h1>Registered Users</h1>

<table>
  <tr>
    <th>S.No</th>
    <th>User Name</th>
    <th>Email</th>
	<th>Mobile</th>
  </tr>
  <%
  if(userData.length!=0){
	  var i=1;
	  userData.forEach(function(data){
	%>
	<tr>
		<td><%=i; %></td>
		<td><%=data.username %></td>
		<td><%=data.email %></td>
		<td><%=data.mobile %></td>
	</tr>
	<% i++; }) %>	
	<% } else{ %>
		<tr>
			<td>Database is empty
			</td>
		</tr>
		<% } %>
</table>
</div>
</body>
</html>

